(declare-fun |increase-x2-decrease-x1 | () Int)
(declare-fun |increase-x2-decrease-x1 pre_cost| () Real)
(declare-fun |increase-x0-decrease-x3 | () Int)
(declare-fun |increase-x0-decrease-x3 pre_cost| () Real)
(assert (>= |increase-x2-decrease-x1 | 0))
(assert (=> (> |increase-x2-decrease-x1 | 0) (= |increase-x2-decrease-x1 pre_cost| 0.0)))
(assert (=> (= |increase-x2-decrease-x1 | 0) (= |increase-x2-decrease-x1 pre_cost| 0.0)))
(assert (>= |increase-x0-decrease-x3 | 0))
(assert (=> (> |increase-x0-decrease-x3 | 0) (= |increase-x0-decrease-x3 pre_cost| 0.0)))
(assert (=> (= |increase-x0-decrease-x3 | 0) (= |increase-x0-decrease-x3 pre_cost| 0.0)))
(assert (let ((a!1 (- (+ 0.0
                 (- 2.0)
                 (* (to_real |increase-x2-decrease-x1 |) 1.0)
                 (- 4.0))
              (* (to_real |increase-x0-decrease-x3 |) (/ 1.0 2.0)))))
  (>= (+ a!1 0.0) 0.0)))
(minimize (+ (* (to_real |increase-x2-decrease-x1 |) 1.0)
   |increase-x2-decrease-x1 pre_cost|
   (* (to_real |increase-x0-decrease-x3 |) 1.0)
   |increase-x0-decrease-x3 pre_cost|))
(check-sat)

