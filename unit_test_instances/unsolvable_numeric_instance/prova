(declare-fun |increase-a-decrease-b | () Real)
(declare-fun |increase-a-decrease-b pre_cost| () Real)
(declare-fun |increase-b-decrease-a | () Real)
(declare-fun |increase-b-decrease-a pre_cost| () Real)
(assert (>= |increase-a-decrease-b | (to_real 0)))
(assert (=> (> |increase-a-decrease-b | (to_real 0))
    (= |increase-a-decrease-b pre_cost| 0.0)))
(assert (=> (= |increase-a-decrease-b | (to_real 0))
    (= |increase-a-decrease-b pre_cost| 0.0)))
(assert (>= |increase-a-decrease-b | (to_real 0)))
(assert (=> (> |increase-a-decrease-b | (to_real 0))
    (= |increase-a-decrease-b pre_cost| 0.0)))
(assert (=> (= |increase-a-decrease-b | (to_real 0))
    (= |increase-a-decrease-b pre_cost| 0.0)))
(assert (>= |increase-b-decrease-a | (to_real 0)))
(assert (=> (> |increase-b-decrease-a | (to_real 0))
    (= |increase-b-decrease-a pre_cost| 0.0)))
(assert (=> (= |increase-b-decrease-a | (to_real 0))
    (= |increase-b-decrease-a pre_cost| 0.0)))
(assert (>= |increase-b-decrease-a | (to_real 0)))
(assert (=> (> |increase-b-decrease-a | (to_real 0))
    (= |increase-b-decrease-a pre_cost| 0.0)))
(assert (=> (= |increase-b-decrease-a | (to_real 0))
    (= |increase-b-decrease-a pre_cost| 0.0)))
(assert (let ((a!1 (+ (- (+ 0.0 0.0 (* |increase-a-decrease-b | 1.0))
                 (* |increase-a-decrease-b | 1.0))
              (* |increase-b-decrease-a | 1.0))))
  (> (+ (- a!1 (* |increase-b-decrease-a | 1.0)) (- 2.0)) 0.0)))
(minimize (+ (* |increase-a-decrease-b | 1.0)
   |increase-a-decrease-b pre_cost|
   (* |increase-b-decrease-a | 1.0)
   |increase-b-decrease-a pre_cost|))
(check-sat)

